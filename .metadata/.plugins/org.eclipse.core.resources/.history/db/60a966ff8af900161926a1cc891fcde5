package testFile;
import java.util.*;
import java.io.*;
 

public class hey {

	public static void main(String[] args) throws FileNotFoundException{
		File wassup = new File("Resources/hi");
		Scanner whatIsGoingOn = new Scanner(new File("Resources/hi"));
		ArrayList<Double> hi = new ArrayList<Double>();
				for(int i = 0; i < wassup.length(); i++){
					hi.add(whatIsGoingOn.nextDouble());
				}
				for(int i = 0; i < wassup.length(); i++){
					System.out.println(hi.get(i+1) - hi.get(i));
				}
	}

}
